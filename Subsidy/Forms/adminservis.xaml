<Window x:Class="Subsidy.Forms.adminservis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Subsidy.Forms"
        mc:Ignorable="d"
        Title="adminservis" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="55*"/>
            <RowDefinition Height="320*"/>
            <RowDefinition Height="44*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="94*"/>
            <ColumnDefinition Width="625*"/>
            <ColumnDefinition Width="73*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Row=" 1" Grid.Column=" 1" Orientation="Vertical" VerticalAlignment="Center">
           
                <Button x:Name="btgetclient" Content="посмотреть список данных заявителей(общая информация) " Click="btgetclient_Click" Height="50" Margin="0,0,10,0" />
             <Button x:Name="btgetclientsubsidy" Content="посмотреть список данных заявителей(субсидия) " Click="btgetclientsubsidy_Click" Height="50" Margin="0,5,10,0" />
           
        </StackPanel>
        <Button x:Name="bthome"  Content="Назад" Click="bthome_Click"/>
    </Grid>
</Window>
